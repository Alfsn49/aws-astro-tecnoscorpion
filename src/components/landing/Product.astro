---
const response= await fetch("https://opensheet.elk.sh/1bQ9GMLrjtvVnmlcPcCXKVMPbXsCbIumJAz3d05CHYIM/1")

const datas = await response.json();

---

<div class="bg-white">
    <div class="mx-auto max-w-2xl px-4 py-16 sm:px-6 sm:py-24 lg:max-w-7xl lg:px-8">
      <h2 class="sr-only">Products</h2>
  
      <div class="grid grid-cols-1 gap-x-6 gap-y-10 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 xl:gap-x-8">
        {
            datas.map((data: any) =>(
            <a href="https://t.me/joao17017" class="group">
          <div class="aspect-h-1 aspect-w-1 w-full overflow-hidden rounded-lg bg-gray-200 xl:aspect-h-8 xl:aspect-w-7">
            <img src={data.Imagen} alt="" class="h-full w-full object-cover object-center group-hover:opacity-75">
          </div>
          <h3 class="mt-4 text-sm text-gray-700">{data.Nombre}</h3>
          <p class="mt-1 text-sm text-gray-500">{data.Descripcion}</p>
          <p class="mt-1 text-lg font-medium text-gray-900">${data.Precio}</p>
        </a>
            ))
        }

      </div>
    </div>
  </div>