---
import {MobileHeader} from '../landing/BannerHeader'
---
<div class="bg-white relative">
    <header class=" inset-x-0 top-0 z-50">
        <nav
          class="flex items-center justify-between p-6 lg:px-8"
          aria-label="Global"
        >
          <div class="flex lg:flex-1">
            <a href="/" class="-m-1.5 p-1.5">
              <span class="sr-only">TecnoScorpion</span>
              <img
                class="h-8 w-auto"
                src="power-logo.png"
                alt=""
              />
            </a>
          </div>
          <div class="hidden lg:flex lg:gap-x-12">
            <a href="/" class="text-sm font-semibold leading-6 text-gray-900"
              >Inicio</a
            >
            <a href="/product" class="text-sm font-semibold leading-6 text-gray-900"
              >Productos</a
            >
            <a href="/about" class="text-sm font-semibold leading-6 text-gray-900"
              >Sobre Nosotros</a
            >
            <a href="/contact" class="text-sm font-semibold leading-6 text-gray-900"
              >Contactenos</a
            >
          </div>
          <MobileHeader client:visible/>
          
        </nav>
        
      </header>
</div>

